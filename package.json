{
  "name": "svg-sample",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "yarn type:check && vite build",
    "serve": "vite preview",
    "type:check": "vue-tsc --noEmit",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,vue}'",
    "format": "prettier 'src/**/*.{js,ts,vue,json}'",
    "format:fix": "prettier --write 'src/**/*.{js,ts,vue,json}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/gif.js": "0.2.2",
    "bulma": "0.9.3",
    "canvg": "3.0.7",
    "detect-browser": "5.2.0",
    "gif.js": "0.2.0",
    "hex-rgb": "5.0.0",
    "makerjs": "0.17.1",
    "opentype.js": "1.3.3",
    "sass": "1.39.0",
    "text-to-svg": "3.1.5",
    "vue": "3.2.2",
    "vue-gtag": "2.0.1",
    "vue-i18n": "9.2.0-beta.3",
    "vue3-colorpicker": "1.0.5"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "2.4.0",
    "@types/text-to-svg": "3.1.1",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "@vitejs/plugin-vue": "1.6.2",
    "@vue/compiler-sfc": "3.2.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-vue": "7.17.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "prettier": "2.4.0",
    "typescript": "4.4.2",
    "vite": "2.5.6",
    "vue-tsc": "0.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "yarn format:fix",
      "yarn lint:fix"
    ]
  }
}
