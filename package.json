{
  "name": "svg-sample",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "yarn type:check && vite build",
    "serve": "vite preview",
    "type:check": "vue-tsc --noEmit",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,vue}'",
    "format": "prettier 'src/**/*.{js,ts,vue,json}'",
    "format:fix": "prettier --write 'src/**/*.{js,ts,vue,json}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@types/gif.js": "0.2.2",
    "bulma": "0.9.4",
    "canvg": "4.0.1",
    "detect-browser": "5.2.1",
    "gif.js": "0.2.0",
    "hex-rgb": "5.0.0",
    "makerjs": "0.17.1",
    "opentype.js": "1.3.3",
    "sass": "1.55.0",
    "text-to-svg": "3.1.5",
    "vue": "3.2.39",
    "vue-gtag": "2.0.1",
    "vue-i18n": "9.2.2",
    "vue3-colorpicker": "1.0.8"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "6.0.2",
    "@types/text-to-svg": "3.1.1",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "@vitejs/plugin-vue": "3.1.0",
    "@vue/compiler-sfc": "3.2.39",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-vue": "9.5.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "vite": "3.1.3",
    "vue-tsc": "0.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "yarn format:fix",
      "yarn lint:fix"
    ]
  }
}
